{
  "name": "americat",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:maurer2/americat.git",
  "author": "Martin",
  "license": "MIT",
  "scripts": {
    "python:virtualenv:create": "virtualenv python-env",
    "python:virtualenv:start": "source python-env/bin/activate",
    "python:install": "pip3 install -r requirements.txt",
    "python:lint": "pylint --rcfile=.pylintrc ./api",
    "python:format": "black ./api",
    "python:data:dump": "python3 ./api/dump.py",
    "python:data:parse": "python3 ./api/parse.py",
    "python:data:map": "python3 ./api/mapper.py",
    "build": "webpack --progress --mode development",
    "build:analyze": "yarn build && webpack-bundle-analyzer --port 4200 './public/stats.json'",
    "watch": "webpack --progress --mode development --watch",
    "start": "webpack-dev-server --config webpack.config.js",
    "lint": "eslint 'src/**/*.svelte'"
  },
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.2.1",
    "css-loader": "^3.2.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "pug": "^2.0.4",
    "style-loader": "^1.0.0",
    "svelte": "^3.12.1",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.1.2",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  }
}
