{
  "name": "americat",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:maurer2/americat.git",
  "author": "Martin",
  "license": "MIT",
  "scripts": {
    "python:setup": "pipenv --three",
    "python:install": "pipenv install --pre",
    "python:lint": "pipenv run pylint --rcfile=.pylintrc ./api",
    "python:format": "pipenv run black ./api",
    "python:info": "pipenv run python -V",
    "python:data:dump": "pipenv run python ./api/dump.py",
    "python:data:parse": "pipenv run python ./api/parser.py",
    "python:data:map": "pipenv run python ./api/mapper.py",
    "build": "webpack --progress --mode development",
    "build:analyze": "yarn build && webpack-bundle-analyzer --port 4200 './public/stats.json'",
    "watch": "webpack --progress --mode development --watch",
    "start": "webpack-dev-server --config webpack.config.js",
    "lint": "eslint 'src/**/*.svelte'",
    "lint:fix": "eslint 'src/**/*.svelte' --fix",
    "slides:start": "cd slides && yarn start",
    "slides:print": "cd slides && yarn build && cd .. && http-server ./slides/ && node ./print-headless.js"
  },
  "dependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "animatewithsass": "^3.2.1",
    "autoprefixer": "^9.6.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^3.2.1",
    "css-loader": "^3.2.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "postcss-load-config": "^2.1.0",
    "pug": "^2.0.4",
    "puppeteer": "^1.20.0",
    "style-loader": "^1.0.0",
    "sugarss": "^2.0.0",
    "svelte": "^3.12.1",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.1.2",
    "svg-url-loader": "^3.0.2",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  }
}
